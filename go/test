(1000, 0010) + (1000, 0010) = (0110, 0111)
(1000, 0010) * 1 = (1000, 0010)
(1000, 0010) * 2 = (0110, 0111)
(1000, 0010) * 3 = (1010, 0101)
(1000, 0010) * 4 = (0001, 0110)
(1000, 0010) * 5 = (1100, 1000)
(1000, 0010) * 6 = (0111, 0110)
(1000, 0010) * 7 = (1111, 1100)
(1000, 0010) * 8 = (0000, 0001)
(1000, 0010) * 9 = (1111, 0011)
(1000, 0010) * 10 = (0111, 0001)
(1000, 0010) * 11 = (1100, 0100)
(1000, 0010) * 12 = (0001, 0111)
(1000, 0010) * 13 = (1010, 1111)
(1000, 0010) * 14 = (0110, 0001)
(1000, 0010) * 15 = (1000, 1010)
